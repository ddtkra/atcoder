#!/usr/bin/env python3
import sys
sys.setrecursionlimit(10000000)
INF = 1<<32


def solve(N: int):

    if N%2 != 0:
        print(0)
        exit()

    ans = 0
    i = 1
    while(5 ** i <= N):
        ans +=  (N//2)//(5**i)
        i += 1
    
    print(ans)

    return



def main():
    def iterate_tokens():
        for line in sys.stdin:
            for word in line.split():
                yield word
    tokens = iterate_tokens()
    N = int(next(tokens))  # type: int
    solve(N)

if __name__ == '__main__':
    main()
