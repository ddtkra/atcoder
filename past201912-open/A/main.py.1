#!/usr/bin/env python3
import sys
sys.setrecursionlimit(10000000)
INF = 1<<32


def solve(S: str):
    s = {'1', '2', '3', '4', '5', '6', '7', '8', '9', '0'}
    if S[0] in s and S[1] in s and S[2] in s:
        print(int(S)*2)
    else:
        print('error')

    return



def main():
    def iterate_tokens():
        for line in sys.stdin:
            for word in line.split():
                yield word
    tokens = iterate_tokens()
    S = next(tokens)  # type: str
    solve(S)

if __name__ == '__main__':
    main()
